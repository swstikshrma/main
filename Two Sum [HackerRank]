#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

    void count(int list[], int n){
    int num;
    scanf("%d", &num);
    for(int i=0;i<n;i++){
        for(int j=i+1;j<n;j++){
            if(list[i]+list[j] == num){
                printf("%d %d",i,j);
            }
           
        }
    }
   
}
int main() {

   int n;
     scanf("%d", &n);
    int list[n];
    for(int i=0;i<n;i++){
        scanf("%d",&list[i]);
        }
   
    count(list,n);
    return 0;
}    
